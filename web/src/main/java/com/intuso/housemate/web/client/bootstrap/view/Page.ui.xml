<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:FlowPanel>
        <b:Navbar position="FIXED_TOP">
            <b:Container>
                <b:NavbarHeader>
                    <b:Image addStyleNames="brand" url="img/nav-brand-50px-high.png"/>
                    <b:NavbarCollapseButton dataTarget="#navbarLinks"/>
                </b:NavbarHeader>
                <b:NavbarCollapse b:id="navbarLinks">
                    <b:NavbarNav>
                        <b:AnchorListItem ui:field="devicesButton">devices</b:AnchorListItem>
                        <b:AnchorListItem ui:field="automationsButton">automations</b:AnchorListItem>
                        <b:AnchorListItem ui:field="applicationsButton">applications</b:AnchorListItem>
                        <b:AnchorListItem ui:field="hardwaresButton">hardware</b:AnchorListItem>
                        <b:AnchorListItem ui:field="usersButton">users</b:AnchorListItem>
                        <b:AnchorListItem ui:field="logoutButton">logout</b:AnchorListItem>
                    </b:NavbarNav>
                </b:NavbarCollapse>
            </b:Container>
        </b:Navbar>
        <b:Container>
            <g:SimplePanel ui:field="container"/>
        </b:Container>
    </g:FlowPanel>
</ui:UiBinder>