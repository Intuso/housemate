<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
             xmlns:h='urn:import:com.intuso.housemate.web.client.bootstrap.widget'>
    <g:FlowPanel>
        <b:Row>
            <b:Column size="8">
                <b:Heading size="5">Executing</b:Heading>
                <b:Icon type="THUMBS_UP" ui:field="executingIcon"/>
                <b:Heading size="5">Properties</b:Heading>
                <h:property.PropertyList ui:field="propertyList"/>
            </b:Column>
        </b:Row>
    </g:FlowPanel>
</ui:UiBinder>