<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
             xmlns:h='urn:import:com.intuso.housemate.web.client.bootstrap.widget'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <b:Row>
        <b:Column size="6" addStyleNames="panel">
            <b:Heading size="3">Shortcuts</b:Heading>
            <g:FlowPanel ui:field="deviceShortcutWidgetContainer"/>
        </b:Column>
        <b:Column size="6" addStyleNames="panel">
            <b:Heading size="3">Devices</b:Heading>
            <h:list.ObjectNavs ui:field="deviceList"/>
            <b:Heading size="3">Automations</b:Heading>
            <h:list.ObjectNavs ui:field="automationList"/>
            <b:Heading size="3">Users</b:Heading>
            <h:list.ObjectNavs ui:field="userList"/>
        </b:Column>
    </b:Row>
</ui:UiBinder>